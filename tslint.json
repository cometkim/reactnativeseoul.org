{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "indent": [true, "spaces", 4],
        "quotemark": [true, "single", "jsx-single"],
        "semicolon": [true, "never"],
        "ordered-imports": false,
        "jsx-no-multiline-js": false,
        "jsx-boolean-value": false
    }
}
